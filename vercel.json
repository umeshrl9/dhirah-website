{
  "version": 2,
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/$1.js"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    },
    {
      "src": "/.*",
      "status": 404,
      "dest": "/404"
    }
  ],
  "trailingSlash": false
}
