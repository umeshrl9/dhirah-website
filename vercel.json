{
  "version": 2,
  "headers": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|gif|svg|css|js)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.js"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}
